{
  "id": null,
  "uid": "core-slos",
  "title": "Core SLOs",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "API Latency (p50/p95)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_ms_bucket{job=\"api\",route=~\"/permits.*|/nlp/listing-writer.*\"}[5m])))",
          "legendFormat": "p95"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.5, sum by (le) (rate(http_request_duration_ms_bucket{job=\"api\",route=~\"/permits.*|/nlp/listing-writer.*\"}[5m])))",
          "legendFormat": "p50"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 250 },
              { "color": "red", "value": 300 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "API Error Rate",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_requests_total{job=\"api\",code=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"api\"}[5m]))",
          "legendFormat": "5xx rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.01 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "API Requests Per Second",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_requests_total{job=\"api\"}[1m]))",
          "legendFormat": "rps"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Search Latency (p95)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_ms_bucket{job=\"search\",route=~\"/search.*\"}[5m])))",
          "legendFormat": "search p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 600 },
              { "color": "red", "value": 800 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Ingestion Runs (success vs error)",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(ingestion_runs_total{result=\"success\"}[6h])",
          "legendFormat": "success"
        },
        {
          "refId": "B",
          "expr": "increase(ingestion_runs_total{result=\"error\"}[6h])",
          "legendFormat": "error"
        }
      ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Ingestion Rows Written (total)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "ingestion_rows_written_total",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Last Ingestion Run",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 12 },
      "targets": [
        {
          "refId": "A",
          "expr": "ingestion_last_run_timestamp",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Service Uptime (5m avg)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(up{job=~\"api|search|ingestion\"}[5m])",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 1
        },
        "overrides": []
      }
    }
  ]
}
